{
  "name": "react-vite-template",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "prod-preview": "vite preview",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "build-storybook": "storybook build -c ./config/storybook",
    "test": "jest --config ./config/jest/jest.config.json",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "prettier": "npx prettier \"src/**/*.{ts,tsx,scss}\" --check",
    "prettier:fix": "npx prettier \"**/*.{ts,tsx,scss}\" --write",
    "prepare": "husky install",
    "chromatic": "npx chromatic --project-token=<YOUR_TOKEN>",
    "generate:slice": "node ./scripts/createFSDSlice/index.js",
    "clear-cache": "ts-node ./scripts/build/clearCache.ts"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.3.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/react-vite": "^7.0.7",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "chromatic": "^6.17.4",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-fsd-arch-validator": "^0.0.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8",
    "sass": "^1.62.0",
    "storybook": "^7.0.7",
    "stylelint": "^15.10.3",
    "stylelint-config-clean-order": "^5.0.1",
    "stylelint-config-standard-scss": "^9.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-fonts": "^0.7.0",
    "vite-plugin-svgr": "^2.4.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "eslint --fix",
    "**/*.scss": "stylelint --fix",
    "**/*.{ts,tsx,scss}": "prettier --write --ignore-unknown"
  },
  "_id": "mavii-web@1.0.0"
}
